.tablist {
  margin: 0;
  padding: 0;
  height: 50px;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

.tab {
  color: #fff;
  display: flex;
  flex: 1 1 auto;
  align-items: center;
  justify-content: center;
  background: #00a6e2;
}

.tabpanel {
  margin: 0;
  margin-bottom: 170px;
}
